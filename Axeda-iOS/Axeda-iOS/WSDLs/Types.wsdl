<?xml version="1.0" encoding="UTF-8"?>

<!-- ================================================================= -->
<!-- Shared Data Types                                                 -->
<!-- ================================================================= -->
<wsdl:definitions targetNamespace="http://sdk.axeda.com/webservices/type/"
                  xmlns="http://schemas.xmlsoap.org/wsdl/"
                  xmlns:apachesoap="http://xml.apache.org/xml-soap"
                  xmlns:impl="http://sdk.axeda.com/webservices/type/"
                  xmlns:intf="http://sdk.axeda.com/webservices/type/"
                  xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/"
                  xmlns:tns1="http://sdk.axeda.com/webservices/type"
                  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                  xmlns:wsdlsoap="http://schemas.xmlsoap.org/wsdl/soap/"
                  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  
  <wsdl:types>
    <schema xmlns="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://sdk.axeda.com/webservices/type"
            xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
            xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/"
            xmlns:typens="http://sdk.axeda.com/webservices/type">

      <simpleType name="ActionType">
        <restriction base="xsd:string">
          <enumeration value="create"/>
          <enumeration value="find"/>
          <enumeration value="createIfNotFound"/>
        </restriction>
      </simpleType>

      <simpleType name="DataItemType">
        <restriction base="xsd:string">
          <enumeration value="string"/>
          <enumeration value="digital"/>
          <enumeration value="analog"/>
        </restriction>
      </simpleType>

      <simpleType name="AlarmState">
        <restriction base="xsd:string">
          <enumeration value="Started"/>
          <enumeration value="Acknowledged"/>          
		  <enumeration value="Escalated"/>
		  <enumeration value="Suppressed"/>
		  <enumeration value="Closed"/>
		  <enumeration value="Duplicate"/>
        </restriction>
      </simpleType>	 

	  <simpleType name="CreateDisposition">
        <restriction base="xsd:string">
          <enumeration value="suppress"/>
          <enumeration value="disable"/>
          <enumeration value="normal"/>
        </restriction>
      </simpleType>

      <simpleType name="CreateDispositionEntity">
        <restriction base="xsd:string">
          <enumeration value="model"/>
          <enumeration value="device"/>
        </restriction>
      </simpleType>
	  	  
      <simpleType name="AddressingMode">
        <restriction base="xsd:string">
          <enumeration value="literal"/>
          <enumeration value="dataItemName"/>
          <enumeration value="dataItemValue"/>
          <enumeration value="geofenceName"/>
        </restriction>
      </simpleType>

      <simpleType name="DataType">
        <restriction base="xsd:string">
          <enumeration value="String"/>
          <enumeration value="Double"/>
          <enumeration value="Integer"/>
          <enumeration value="Boolean"/>
          <enumeration value="Date"/>
        </restriction>
      </simpleType>

      <simpleType name="BooleanOperator">
        <restriction base="xsd:string">
          <enumeration value="and"/>
          <enumeration value="or"/>
          <enumeration value="not"/>
        </restriction>
      </simpleType>

      <simpleType name="RelationalOperator">
        <restriction base="xsd:string">
          <enumeration value="EQUAL_TO"/>
          <enumeration value="NOT_EQUAL_TO"/>
          <enumeration value="LESS_THAN"/>
          <enumeration value="GREATER_THAN"/>
          <enumeration value="GRT_THAN_EQUAL_TO"/>
          <enumeration value="LESS_THAN_EQUAL_TO"/>
          <enumeration value="LIKE"/>
        </restriction>
      </simpleType>

      <complexType name="StringList">
        <complexContent>
          <restriction base="soapenc:Array">
            <attribute ref="soapenc:arrayType" wsdl:arrayType="xsd:string[]"/>
          </restriction>
        </complexContent>
      </complexType>

      <complexType name="AddressingModeList">
        <complexContent>
          <restriction base="soapenc:Array">
            <attribute ref="soapenc:arrayType" wsdl:arrayType="tns1:AddressingMode[]"/>
          </restriction>
        </complexContent>
      </complexType>

      <simpleType name="PrimitiveType">
        <restriction base="xsd:string">
          <enumeration value="string"/>
          <enumeration value="digital"/>
          <enumeration value="integer"/>
          <enumeration value="analog"/>
        </restriction>
      </simpleType>

      <complexType name="PrimitiveTypeList">
        <complexContent>
          <restriction base="soapenc:Array">
            <attribute ref="soapenc:arrayType" wsdl:arrayType="tns1:PrimitiveType[]"/>
          </restriction>
        </complexContent>
      </complexType>

      <simpleType name="SCMOperator">
        <restriction base="xsd:string">
          <enumeration value="EQUAL_TO"/>
          <enumeration value="NOT_EQUAL_TO"/>
          <enumeration value="LESS_THAN"/>
          <enumeration value="GREATER_THAN"/>
          <enumeration value="GRT_THAN_EQUAL_TO"/>
          <enumeration value="LESS_THAN_EQUAL_TO"/>
        </restriction>
      </simpleType>
      
      <simpleType name="DataItemQueryOperator">
        <restriction base="xsd:string">
          <enumeration value="EQUAL_TO"/>
          <enumeration value="NOT_EQUAL_TO"/>
          <enumeration value="LESS_THAN"/>
          <enumeration value="GREATER_THAN"/>
          <enumeration value="GRT_THAN_EQUAL_TO"/>
          <enumeration value="LESS_THAN_EQUAL_TO"/>
          <enumeration value="HAS"/>
        </restriction>
      </simpleType>

      <simpleType name="DeviceStatus">
        <restriction base="xsd:string">
          <enumeration value="CONNECTED"/>
          <enumeration value="MISSING"/>
          <enumeration value="PARTIALLY_CONNECTED"/>
		      <enumeration value="COMPLETELY_DISCONNECTED"/>
        </restriction>
      </simpleType>

       <complexType name="DeviceDataItemQuery">
        <sequence>
        <element name="name" nillable="true" type="xsd:string"/>
        <element name="value" nillable="true" type="xsd:string"/>
        <element name="dataItemQueryOperator" nillable="true" type="tns1:DataItemQueryOperator"/>
        </sequence>
      </complexType>

      <complexType name="NamedPackageQuery">
        <sequence>
          <element name="id" nillable="true" type="xsd:long"/>
          <element name="name" nillable="true" type="xsd:string"/>
        </sequence>
      </complexType>

      <complexType name="RegistryDependencyEntryQuery">
        <sequence>
          <element name="modelId" nillable="true" type="xsd:long"/>
        </sequence>
      </complexType>

      <complexType name="IdList">
        <complexContent>
          <restriction base="soapenc:Array">
            <attribute ref="soapenc:arrayType" wsdl:arrayType="xsd:long[]"/>
          </restriction>
        </complexContent>
      </complexType>

      <complexType name="SessionInfo">
        <sequence>
          <element name="User" nillable="true" type="xsd:string"/>
          <element name="Password" nillable="true" type="xsd:string"/>
          <xsd:element name="sessionID" nillable="true" type="xsd:string"/>
        </sequence>
      </complexType>

      <complexType name="DeviceSessionInfo">
        <sequence>
          <element name="Model" nillable="true" type="xsd:string"/>
          <element name="Serial" nillable="true" type="xsd:string"/>
          <element name="Token" nillable="true" type="xsd:string"/>
          <element name="sessionID" nillable="true" type="xsd:string"/>
        </sequence>
      </complexType>

      <complexType name="UserOrDeviceSessionInfo">
        <sequence>
          <element name="SessionInfo" nillable="true"
                   type="tns1:SessionInfo"/>
          <element name="DeviceSessionInfo" nillable="true"
                   type="tns1:DeviceSessionInfo"/>
        </sequence>
      </complexType>

      <simpleType name="CompressionType">
        <restriction base="xsd:string">
          <enumeration value="NONE"/>
          <enumeration value="TAR_GZIP"/>
        </restriction>
      </simpleType>

      <complexType name="Version">
        <sequence>
          <element name="rev" nillable="false" type="xsd:int"/>
          <element name="minor" nillable="false" type="xsd:int"/>
          <element name="build" nillable="false" type="xsd:int"/>
          <element name="major" nillable="false" type="xsd:int"/>
        </sequence>
      </complexType>

      <complexType name="UserGroup">
        <sequence>
          <element name="id" nillable="true" type="xsd:long"/>
          <element name="name" nillable="true" type="xsd:string"/>
          <element name="description" nillable="true" type="xsd:string"/>
        </sequence>
      </complexType>

      <complexType name="UserGroupList">
        <complexContent>
          <restriction base="soapenc:Array">
            <attribute ref="soapenc:arrayType"
                       wsdl:arrayType="tns1:UserGroup[]"/>
          </restriction>
        </complexContent>
      </complexType>

      <complexType name="DataItem">
        <sequence>
          <element name="id" type="xsd:long"/>
          <element name="name" nillable="true" type="xsd:string"/>
          <element name="description" nillable="true" type="xsd:string"/>
          <element name="alias" nillable="true" type="xsd:string"/>
          <element name="model" nillable="true" type="xsd:string"/>
          <element name="visible" nillable="true" type="xsd:boolean"/>
          <element name="stored" nillable="true" type="xsd:boolean"/>
          <element name="readOnly" nillable="true" type="xsd:boolean"/>
          <element name="type" nillable="true" type="tns1:DataItemType"/>
          <element name="unit" nillable="true" type="xsd:string"/>
        </sequence>
      </complexType>

      <complexType name="Location">
        <sequence>
          <element name="id" type="xsd:long"/>
          <element name="action" nillable="true" type="tns1:ActionType"/>
          <element name="name" nillable="true" type="xsd:string"/>
          <element name="line1" nillable="true" type="xsd:string"/>
          <element name="line2" nillable="true" type="xsd:string"/>
          <element name="city" nillable="true" type="xsd:string"/>
          <element name="state" nillable="true" type="xsd:string"/>
          <element name="zip" nillable="true" type="xsd:string"/>
          <element name="country" nillable="true" type="xsd:string"/>
        </sequence>
      </complexType>

      <complexType name="Organization">
        <sequence>
          <element name="id" type="xsd:long"/>
          <element name="action" nillable="true" type="tns1:ActionType"/>
          <element name="name" nillable="true" type="xsd:string"/>
          <element name="description" nillable="true" type="xsd:string"/>
          <element name="location" nillable="true" type="tns1:Location"/>
        </sequence>
      </complexType>

      <complexType name="DeviceList">
        <complexContent>
          <restriction base="soapenc:Array">
            <attribute ref="soapenc:arrayType" wsdl:arrayType="tns1:Device[]"/>
          </restriction>
        </complexContent>
      </complexType>

      <complexType name="DeviceSystem">
        <sequence>
          <element name="id" type="xsd:long"/>
          <element name="action" nillable="true" type="tns1:ActionType"/>
          <element name="name" nillable="true" type="xsd:string"/>
          <element name="description" nillable="true" type="xsd:string"/>
          <element name="devices" nillable="true" type="tns1:DeviceList"/>
        </sequence>
      </complexType>


      <complexType name="Condition">
        <sequence>
          <element name="action" nillable="true" type="tns1:ActionType"/>
          <element name="name" nillable="true" type="xsd:string"/>
          <element name="priority" type="xsd:int"/>
        </sequence>
      </complexType>

      <complexType name="Device">
        <sequence>
          <element name="id" type="xsd:long"/>
          <element name="name" nillable="true" type="xsd:string"/>
          <element name="serialNumber" nillable="true" type="xsd:string"/>
          <element name="model" nillable="true" type="xsd:string"/>
          <element name="pingRate" nillable="true" type="xsd:long"/>
          <element name="missing" type="xsd:boolean"/>
          <element name="lastContactDate" nillable="true" type="xsd:dateTime"/>
          <element name="registrationDate" nillable="true" type="xsd:dateTime"/>
          <element name="location" nillable="true" type="tns1:Location"/>
          <element name="customer" nillable="true" type="tns1:Organization"/>
          <element name="deviceSystem" nillable="true"
                   type="tns1:DeviceSystem"/>
          <element name="condition" nillable="true" type="tns1:Condition"/>
          <element name="associatedGatewayId" nillable="true" type="xsd:long"/>
          <element name="currentMobileLocation" nillable="true" type="tns1:MobileLocation"/>
          <element name="deviceFamily" nillable="true" type="xsd:string"/>
          <element name="agentVersion" nillable="true" type="xsd:string"/>
          <element name="timezone" nillable="true" type="xsd:string"/>
        </sequence>
      </complexType>

      <simpleType name="PackageType">
        <restriction base="xsd:string">
          <enumeration value="USER_CREATED"/>
          <enumeration value="UPLOAD_ACTION"/>
          <enumeration value="UPLOAD_REQUEST"/>
          <enumeration value="SYSTEM_CREATED"/>
          <enumeration value="DOWNLOAD_ACTION"/>
          <enumeration value="NAMED_INSTRUCTION_SET"/>
        </restriction>
      </simpleType>

      <complexType name="SCMStatus">
        <sequence>
          <element name="id" type="xsd:long"/>
          <element name="status" type="xsd:string"/>
        </sequence>
      </complexType>

      <complexType name="DeployedPackageQuery">
        <sequence>
          <element name="userId" nillable="true" type="xsd:long"/>
          <element name="deviceId" nillable="true" type="xsd:long"/>
          <!--<element name="endDate" nillable="true" type="xsd:dateTime"/>-->
          <element name="name" nillable="true" type="xsd:string"/>
          <element name="version" nillable="true" type="xsd:string"/>
          <element name="description" nillable="true" type="xsd:string"/>
          <element name="uploadedFilename" nillable="true" type="xsd:string"/>
          <!--<element name="startDate" nillable="true" type="xsd:dateTime"/>-->
          <element name="status" nillable="true" type="tns1:SCMStatus"/>
        </sequence>
      </complexType>

      <simpleType name="SCMStatusConstants">
        <restriction base="xsd:string">
          <enumeration value="QUEUED"/>
          <enumeration value="STARTED"/>
          <enumeration value="FINISHED_OK"/>
          <enumeration value="FINISHED_ERROR"/>
          <enumeration value="CANCELED"/>
          <enumeration value="TIMED_OUT"/>
          <enumeration value="MISSING"/>
          <enumeration value="ROLLING_BACK"/>
          <enumeration value="ROLLBACK_ERROR"/>
          <enumeration value="ROLLBACK_SUCCESS"/>
          <enumeration value="DEPENDENCY_FAILED"/>
          <enumeration value="CANCEL_PENDING"/>
          <enumeration value="PENDING_ASK"/>
          <enumeration value="PERMISSION_DENIED"/>
          <enumeration value="UPLOAD_INHIBITED"/>
          <enumeration value="PAUSED"/>
          <enumeration value="PAUSE_PENDING"/>
          <enumeration value="PREEMPTED"/>
          <enumeration value="DUPLICATE_INVISIBLE_SCRIPT_NAME"/>
          <enumeration value="NONE"/>
          <enumeration value="RUNNING"/>
          <enumeration value="RUN_PENDING"/>
          <enumeration value="REMOVE_PENDING"/>
          <enumeration value="DEPLOY_PENDING"/>
          <enumeration value="DEPLOY_ERROR"/>
          <enumeration value="DISABLE_PENDING"/>
          <enumeration value="DISABLED"/>
          <enumeration value="ENABLE_PENDING"/>
          <enumeration value="REMOVE_ERROR"/>
          <enumeration value="ASSOCIATE_PENDING"/>
          <enumeration value="ASSOCIATE_ERROR"/>
          <enumeration value="DISSOCIATE_PENDING"/>
          <enumeration value="DISSOCIATE_ERROR"/>
          <enumeration value="NOT_RUNNING"/>
          <enumeration value="COMPLETE"/>
          <enumeration value="SUCCESS"/>
          <enumeration value="ERROR"/>
          <enumeration value="INHIBITED_BY_AGENT"/>
          <enumeration value="NOT_RETRIEVED"/>
          <enumeration value="NOT_PROCESSED"/>
          <enumeration value="DENIED"/>
          <enumeration value="DENIED_TIMEOUT"/>
          <enumeration value="UNSUPPORTED"/>
          <enumeration value="ASKING"/>
          <enumeration value="ASK_DENIED"/>
          <enumeration value="ASK_ACCEPTED"/>
          <enumeration value="DENIED_APMOFFLINE"/>
          <enumeration value="SCRIPT_BAD_STATE"/>
          <enumeration value="SCRIPT_RUNNING"/>
          <enumeration value="SCRIPT_NO_SEQUENCE_FILE"/>
          <enumeration value="SCRIPT_NOT_FOUND"/>
          <enumeration value="SCRIPT_DUPLICATE_NAME"/>
          <enumeration value="SCRIPT_NOT_RUNNING"/>
          <enumeration value="SCRIPT_INVALID_COMMAND"/>
          <enumeration value="SCRIPT_BAD_XML"/>
          <enumeration value="SCRIPT_EMPTY_NAME"/>
          <enumeration value="SCRIPT_WAITING"/>
          <enumeration value="SCRIPT_DISABLED"/>
          <enumeration value="SCRIPT_ALREADY_SCHEDULED"/>
          <enumeration value="SCRIPT_NOT_SCHEDULED"/>
          <enumeration value="SCRIPT_NOT_DYNAMIC"/>
          <enumeration value="SCRIPT_FINISHED_WITH_ERROR"/>
          <enumeration value="SCRIPT_DUPLICATE_INVISIBLE_NAME"/>
          <enumeration value="SCRIPT_DISABLED_"/>
          <enumeration value="SCRIPT_ALREADY_SCHEDULED_"/>
          <enumeration value="SCRIPT_NOT_SCHEDULED_"/>
          <enumeration value="SCRIPT_NOT_DYNAMIC_"/>
          <enumeration value="SCRIPT_FINISHED_WITH_ERROR_"/>
          <enumeration value="FAILED"/>
          <enumeration value="INVALID_PARAMETERS"/>
          <enumeration value="FILE_OPEN_ERROR"/>
          <enumeration value="FILE_READ_ERROR"/>
          <enumeration value="BAD_FILE_FORMAT"/>
          <enumeration value="NOT_IMPLEMENTED"/>
          <enumeration value="OPERATION_TIMED_OUT"/>
          <enumeration value="DATA_ITEM_READ_ONLY"/>
          <enumeration value="HANDLE_OPEN_ERROR"/>
          <enumeration value="CONVERSION_ERROR"/>
          <enumeration value="TYPE_MISMATCH"/>
          <enumeration value="OBJECT_NOT_FOUND"/>
          <enumeration value="INVALID_HANDLE"/>
          <enumeration value="INVALID_NAME"/>
          <enumeration value="ALREADY_EXISTS"/>
          <enumeration value="BAD_QUALITY"/>
          <enumeration value="OUT_OF_RANGE"/>
          <enumeration value="UNKNOWN_METHOD"/>
          <enumeration value="BAD_SOAP_FORMAT"/>
          <enumeration value="BAD_XML_FORMAT"/>
          <enumeration value="DATA_ITEM_NOT_FOUND"/>
          <enumeration value="AGENT_AUTH_IN_PROGRESS"/>
          <enumeration value="AGENT_AUTH_PROVIDER_UNKNOWN"/>
          <enumeration value="AGENT_AUTH_PROVIDER_UNAVAILABLE"/>
          <enumeration value="AGENT_AUTH_ACCESS_DENIED"/>
          <enumeration value="AGENT_AUTH_NEED_MORE_CREDENTIALS"/>
          <enumeration value="WAITING_TO_DELIVER_TO_AGENT"/>
          <enumeration value="DELIVERED_TO_AGENT"/>
          <enumeration value="EXPORTED"/>
          <enumeration value="EXPORTED_FINISHED"/>
        </restriction>
      </simpleType>

      <complexType name="DeployedPackage">
        <sequence>
          <element name="id" type="xsd:long"/>
          <element name="interval" nillable="false" type="xsd:long"/>
          <element name="packageId" nillable="false" type="xsd:long"/>
          <element name="delayed" nillable="true" type="xsd:date"/>
          <element name="devices" nillable="false" type="tns1:DeviceList"/>
          <element name="immediate" nillable="false" type="xsd:boolean"/>
          <element name="notifyOnError" nillable="true" type="xsd:boolean"/>
          <element name="notifyOnCompletion" nillable="true" type="xsd:boolean"/>
          <element name="emailAddresses" nillable="true" type="xsd:string"/>
          <element name="soapId" nillable="true" type="xsd:long"/>
          <element name="status" nillable="true" type="tns1:SCMStatus"/>
          <element name="deployedBy" nillable="true" type="xsd:string"/>
          <element name="deployTime" nillable="true" type="xsd:dateTime"/>
          <element name="completionTime" nillable="true" type="xsd:dateTime"/>
        </sequence>
      </complexType>

      <complexType name="DeployedPackageList">
        <complexContent>
          <restriction base="soapenc:Array">
            <attribute ref="soapenc:arrayType"
                       wsdl:arrayType="tns1:DeployedPackage[]"/>
        </restriction>
      </complexContent>
    </complexType>

      <!-- start of instruction -->
      <complexType name="Instruction">
        <sequence>
          <element name="id" type="xsd:long"/>
        </sequence>
      </complexType>

      <complexType name="DownloadInstruction">
        <complexContent>
          <extension base="tns1:Instruction">
            <sequence>
              <element name="destinationDirectory" type="xsd:string"/>
              <element name="filename" type="xsd:string"/>
              <element name="uploadedFilePath" type="xsd:string"/>
              <element name="hashValue" nillable="true" type="xsd:string"/>
              <element name="fileSize" nillable="true" type="xsd:long"/>
              <element name="compressedFileSize" nillable="true"
                       type="xsd:long"/>
              <element name="execute" nillable="true" type="xsd:boolean"/>
              <element name="overwrite" nillable="true" type="xsd:boolean"/>
              <element name="userCompressed" nillable="true"
                       type="xsd:boolean"/>
              <element name="deviceRelativePath" nillable="true"
                       type="xsd:boolean"/>
              <!--<element name="file" nillable="true" type="xsd:file"/>-->
              <element name="compressionType" nillable="true"
                       type="tns1:CompressionType"/>
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="ExecuteInstruction">
        <complexContent>
          <extension base="tns1:Instruction">
            <sequence>
              <element name="arguments" nillable="true" type="xsd:string"/>
              <element name="application" type="xsd:string"/>
              <element name="asynchronous" nillable="true" type="xsd:boolean"/>
              <element name="deviceRelativePath" nillable="true"
                       type="xsd:boolean"/>
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="RegisterScriptInstruction">
        <complexContent>
          <extension base="tns1:Instruction">
            <sequence>
              <element name="scriptName" type="xsd:string"/>
              <element name="scriptFileName" type="xsd:string"/>
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="RestartInstruction">
        <complexContent>
          <extension base="tns1:Instruction">
            <sequence>
              <element name="hardRestart" type="xsd:boolean"/>
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="RunScriptInstruction">
        <complexContent>
          <extension base="tns1:Instruction">
            <sequence>
              <element name="scriptName" type="xsd:string"/>
              <element name="parameter" nillable="true" type="xsd:string"/>
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="SetDataItemInstruction">
        <complexContent>
          <extension base="tns1:Instruction">
            <sequence>
              <element name="dataItem" type="tns1:DataItem"/>
              <element name="dataItemValue" type="xsd:string"/>
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="UnregisterScriptInstruction">
        <complexContent>
          <extension base="tns1:Instruction">
            <sequence>
              <element name="scriptName" type="xsd:string"/>
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="UploadInstructionEntry">
        <complexContent>
          <extension base="tns1:Instruction">
            <sequence>
              <element name="fileMask" type="xsd:string"/>
              <element name="deleteAfter" nillable="true" type="xsd:boolean"/>
              <element name="deviceRelativePath" nillable="true"
                       type="xsd:boolean"/>
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="UploadInstructionEntryList">
        <complexContent>
          <restriction base="soapenc:Array">
            <attribute ref="soapenc:arrayType"
                       wsdl:arrayType="tns1:UploadInstructionEntry[]"/>
          </restriction>
        </complexContent>
      </complexType>

      <complexType name="UploadInstruction">
        <complexContent>
          <extension base="tns1:Instruction">
            <sequence>
              <element name="entries" type="tns1:UploadInstructionEntryList"/>
              <element name="hint" nillable="true" type="xsd:string"/>
              <element name="clientId" nillable="true" type="xsd:string"/>
              <element name="sessionId" nillable="true" type="xsd:long"/>
              <element name="associationId" nillable="true" type="xsd:long"/>
              <element name="partialFilePosition" nillable="true"
                       type="xsd:long"/>
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="WaitInstruction">
        <complexContent>
          <extension base="tns1:Instruction">
            <sequence>
              <element name="dependencyList"
                       type="tns1:DataItemDependencyList"/>
              <element name="timeout" type="xsd:int"/>
            </sequence>
          </extension>
        </complexContent>
      </complexType>

      <complexType name="DownloadInstructionList">
        <complexContent>
          <restriction base="soapenc:Array">
            <attribute ref="soapenc:arrayType"
                       wsdl:arrayType="tns1:DownloadInstruction[]"/>
          </restriction>
        </complexContent>
      </complexType>

      <complexType name="ExecuteInstructionList">
        <complexContent>
          <restriction base="soapenc:Array">
            <attribute ref="soapenc:arrayType"
                       wsdl:arrayType="tns1:ExecuteInstruction[]"/>
          </restriction>
        </complexContent>
      </complexType>

      <complexType name="RegisterScriptInstructionList">
