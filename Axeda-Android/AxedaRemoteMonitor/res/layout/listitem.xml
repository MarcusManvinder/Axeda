<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
android:id="@+id/widget306"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:layout_marginBottom="30dip"
xmlns:android="http://schemas.android.com/apk/res/android"
>
<TextView
android:id="@+id/alarmname"
android:layout_width="190px"
android:layout_height="44px"
android:text="There is a fire in the warehouse tonight"
android:textSize="17px"
android:textStyle="bold"
android:textColor="@color/white"
android:layout_x="45px"
android:layout_y="0px"
>
</TextView>
<TextView
android:id="@+id/date"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:typeface="monospace"
android:textColor="@color/white"
android:layout_x="60px"
android:layout_y="47px"
>
</TextView>
<TextView
android:id="@+id/asset"
android:layout_width="wrap_content"
android:layout_height="19px"
android:text="date"
android:textStyle="bold|italic"
android:textColor="@color/blue"
android:layout_y="0px"
android:layout_x="240px">
</TextView>
<ImageView
android:id="@+id/image"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_x="3px" android:layout_y="15px">
</ImageView>
</AbsoluteLayout>
