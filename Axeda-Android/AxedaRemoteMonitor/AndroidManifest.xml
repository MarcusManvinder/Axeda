<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.axeda.monitor" android:versionName="1.0.0" android:versionCode="11">

	<application android:name=".AxedaMonitor" android:label="@string/app_name" android:icon="@drawable/appicon">
		<uses-library android:name="com.google.android.maps" />
		<activity android:name=".Login" android:label="@string/app_name"
			android:screenOrientation="portrait">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".Main" android:screenOrientation="portrait" />
		<activity android:name=".MyCurrentLocation"
			android:screenOrientation="portrait" />
		<activity android:name=".DetailsTab"
			android:screenOrientation="portrait" />
		<activity android:name=".AlarmsTab"
			android:screenOrientation="portrait" />
		<activity android:name=".DataTab" android:screenOrientation="portrait" />
		<activity android:name=".TabBar" android:screenOrientation="portrait" />

	</application>

	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

	<uses-sdk android:minSdkVersion="3"></uses-sdk>

</manifest>

